{
    "name": "tripster",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build:prod": "ng build --prod",
        "build:ci": "yarn && yarn run lint && yarn run test && yarn run build:prod",
        "test": "ng test",
        "lint": "ng lint --fix true",
        "e2e": "ng e2e",
        "build:prod:ssr": "yarn run build:prod:client-and-server-bundles && yarn run webpack:server",
        "build:ssr": "yarn run build:client-and-server-bundles && yarn run webpack:server",
        "serve:ssr": "node dist/server.js",
        "build:prod:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
        "build:client-and-server-bundles": "ng build && ng build --app 1 --output-hashing=false",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "prod:ssr": "yarn run build:prod:ssr && yarn run serve:ssr",
        "ssr": "yarn run build:ssr && yarn run serve:ssr"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "1.0.0-beta.2",
        "@angular/animations": "^5.2.10",
        "@angular/cdk": "^5.2.5",
        "@angular/common": "^5.2.10",
        "@angular/compiler": "^5.2.10",
        "@angular/core": "^5.2.10",
        "@angular/forms": "^5.2.10",
        "@angular/http": "^5.2.10",
        "@angular/material": "^5.2.5",
        "@angular/platform-browser": "^5.2.10",
        "@angular/platform-browser-dynamic": "^5.2.10",
        "@angular/platform-server": "^5.2.10",
        "@angular/router": "^5.2.10",
        "@firebase/app": "^0.2.0",
        "@firebase/auth": "^0.4.2",
        "@firebase/database": "^0.2.2",
        "@ngrx/effects": "^5.2.0",
        "@ngrx/router-store": "^5.2.0",
        "@ngrx/store": "^5.2.0",
        "@ngrx/store-devtools": "^5.2.0",
        "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
        "angularfire2": "5.0.0-rc.6",
        "core-js": "^2.4.1",
        "firebase": "^4.13.0",
        "ngrx-store-freeze": "~0.2.0",
        "ngrx-store-logger": "~0.2.0",
        "rxjs": "^5.5.10",
        "ts-loader": "3.5.0",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@firebase/app-types": "^0.1.1",
        "@angular/cli": "~1.7.3",
        "@angular/compiler-cli": "^5.2.10",
        "@angular/language-service": "^5.2.10",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "tslint-config-airbnb": "^5.8.0",
        "typescript": "~2.5.3"
    }
}